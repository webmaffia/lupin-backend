{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text"
    },
    "slug": {
      "type": "uid"
    },
    "media": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "excerpt": {
      "type": "richtext"
    },
    "content": {
      "type": "richtext"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "articles"
    },
    "publishedOn": {
      "type": "datetime"
    },
    "video": {
      "type": "string"
    },
    "link": {
      "type": "string"
    }
  }
}
